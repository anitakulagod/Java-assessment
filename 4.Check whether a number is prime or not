public class PrimeCheck {
    public static void main(String[] args) {
        int n = 29;
        boolean prime = true;

        for(int i = 2; i <= Math.sqrt(n); i++){
            if(n % i == 0){
                prime = false;
                break;
            }
        }

        System.out.println(n + (prime ? " is Prime" : " is NOT Prime"));
    }
}
